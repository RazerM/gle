

sub test_string_comparison switch1$ switch2$
  if (switch1$ = 'true') and (switch2$ = 'true') then
   return 'both'
  end if
  if (switch1$ = 'true') and (switch2$ = 'false') then
   return 'only switch1'
  end if
  if ('false' = switch1$) and (switch2$ = 'true') then
   return 'only switch2'
  end if
  if (switch1$ = 'false') and (switch2$ = 'false') then
   return 'none'
  end if
  return 'err'
end sub
